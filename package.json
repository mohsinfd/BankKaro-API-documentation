    "react-admin": "^4.11.1",
    "react-dom": "18.2.0",
    "react-router-dom": "^7.6.2",
    "react-transition-group": "^4.4.5",
    "set-cookie-parser": "^2.7.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.6.0",
